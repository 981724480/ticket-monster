<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0"
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
      http://java.sun.com/xml/ns/javaee
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
      
   <name>ticketmonster</name>

   
   <converter>
      <converter-for-class>org.jboss.jdf.example.ticketmonster.model.EventCategory</converter-for-class>
      <converter-class>org.jboss.jdf.example.ticketmonster.jsf.CategoryConverter</converter-class>
   </converter>
   
   <converter>
      <converter-for-class>org.jboss.jdf.example.ticketmonster.model.Venue</converter-for-class>
      <converter-class>org.jboss.jdf.example.ticketmonster.jsf.VenueConverter</converter-class>
   </converter>

</faces-config>
